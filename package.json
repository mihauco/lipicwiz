    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "gh-pages": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.2.0"
